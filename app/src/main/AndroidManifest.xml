<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.paparazziapps.securityapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_paparazzi"
        android:roundIcon="@mipmap/ic_launcher_paparazzi_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Securityapp">
        <activity
            android:name=".SplashActivity"
            android:exported="true">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name=".MainActivity"
            android:label="PaparazziApps - insecure app"
            android:exported="true">
        </activity>
    </application>

</manifest>